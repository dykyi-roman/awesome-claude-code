{
    "name": "dykyi/awesome-claude-code",
    "description": "A collection of awesome Claude Code resources: commands, skills, agents, hooks, and more",
    "type": "composer-plugin",
    "license": "MIT",
    "keywords": [
        "claude",
        "claude-code",
        "anthropic",
        "ai",
        "commands",
        "skills",
        "agents",
        "hooks",
        "mcp",
        "awesome"
    ],
    "authors": [
        {
            "name": "Dykyi",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/dykyi-roman/awesome-claude-code",
    "support": {
        "issues": "https://github.com/dykyi-roman/awesome-claude-code/issues",
        "source": "https://github.com/dykyi-roman/awesome-claude-code"
    },
    "require": {
        "php": ">=8.1",
        "composer-plugin-api": "^2.0"
    },
    "require-dev": {
        "composer/composer": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Dykyi\\AwesomeClaudeCode\\": "src/"
        }
    },
    "extra": {
        "class": "Dykyi\\AwesomeClaudeCode\\ComposerPlugin"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
